<Window x:Class="lab4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab4"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="600">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button x:Name="Button1_1" Content="SelectSort" Click="Button1_1_Click" Margin="5" Width="127"/>
            <Button x:Name="Button1_2" Content="BubbleSort" Click="Button1_2_Click" Margin="5" Width="127"/>
            <Button x:Name="Button1_3" Content="QuickSort" Click="Button1_3_Click" Margin="5" Width="127"/>
            <Button x:Name="Button1_4" Content="TimSort" Click="Button1_4_Click" Margin="5" Width="127"/>
        </StackPanel>
        <Button x:Name="Button2" Content="ExternalSort" Click="Button2_Click" Margin="10"/>
        <Button x:Name="Button3" Content="TaskThree" Click="Button3_Click" Margin="10"/>
    </StackPanel>
</Window>
